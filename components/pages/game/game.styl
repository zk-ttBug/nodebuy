.game-search-btn 
  line-height: 50px;

.pagination-layout 
  font-family: Arial,"Helvetica Neue",Helvetica,"Microsoft YaHei",FreeSans,Arimo,"Droid Sans","wenquanyi micro hei","Hiragino Sans GB","Hiragino Sans GB W3",sans-serif;
  padding: 20px;

.vui-pagination 
  margin: 0;
  padding: 0;

.vui-pagination > li 
  display: table-cell;
  list-style: none;

.vui-pagination > .vui-active > a 
  background: #42a5f5;
  border-color: #42a5f5;
  color: #fff;

.vui-pagination > li > a 
  display: block;
  margin-right: 6px;
  border: 1px solid #999;
  padding: 1px 7px;
  color: #999;
  font-size: 15px;
  text-decoration: none;
  border-radius: 0;

.fr 
  float: right;

.clear-float
  clear: both;